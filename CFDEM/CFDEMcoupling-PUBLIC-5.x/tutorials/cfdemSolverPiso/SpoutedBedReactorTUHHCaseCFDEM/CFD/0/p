FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    outlet
    {
        type               fixedValue;
        value              uniform 0;
    }
    casing
    {
        type               zeroGradient;
    }
    inlet_1
    {
        type               zeroGradient;
    }
    inlet_2
    {
        type               zeroGradient;
    }
}

